@model EntityLayer.Concrete.AnimalHouse
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "AddHouse";
    Layout = "~/Views/Shared/Layouts/_AdminLayout.cshtml";
}
<h1>Barınak Ekle</h1>

<form method="post" class="form-group">
    <label>Barınak Adı</label>
    <input asp-for="Name" class="form-control" />
    <label>Barınak Adresi</label>
    <input asp-for="Adress" class="form-control" />
    <input type="submit" value="Ekle" class="btn btn-outline-dark" />
</form>

